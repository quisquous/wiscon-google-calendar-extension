{
  "manifest_version": 2,

  "name": "WisCon program helper for Google Calendarâ„¢",
  "description": "Adds buttons for adding WisCon events to your calendar.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://wiscon.piglet.org/program/detail*"],
      "js": ["addtocalendar.js"]
    },
    {
      "matches": ["http://wiscon.info/program/detail*"],
      "js": ["addtocalendar.js"]
    },
    {
      "matches": ["http://account.wiscon.net/program/detail*"],
      "js": ["addtocalendar.js"]
    }
  ],

  "icons": {
    "128": "icon_128.png"
  }
}
